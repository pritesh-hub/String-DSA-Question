#include<iostream>
#include<unordered_map>
using namespace std;

void countOccurrence(string str)
{
    unordered_map<char, int> freq;

    for (char ch : str)
    {
        freq[ch]++;
    }

    for (pair<char, int>& pair : freq)
    {
        cout << pair.first << ": " << pair.second << endl;
    }
}

int main()
{
    string str = "programming";
    countOccurrence(str);
    return 0;
}
